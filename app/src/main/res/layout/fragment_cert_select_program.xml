<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.fragments.mentors_programs_fragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/group_368"
        android:layout_width="match_parent"
        android:layout_height="24.47dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="20dp"
        android:background="#00000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <!-- 6:25 -->
        <TextView
            android:id="@+id/some_id"
            android:layout_width="25dp"
            android:layout_height="23.7dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:gravity="top"
            android:text="@string/some_id"
            android:textAppearance="@style/some_id"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <!-- Frame 310 -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame_310"
            android:layout_width="67.90dp"
            android:layout_height="23.06dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <!-- 5G -->
            <TextView
                android:id="@+id/g"
                android:layout_width="17dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="0.2dp"
                android:layout_marginTop="3.75dp"
                android:layout_marginBottom="3.75dp"
                android:gravity="center"

                android:text="@string/g"
                android:textAppearance="@style/g"
                android:textColor="@color/mms_black_2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />


            <!-- fa:wifi -->
            <RelativeLayout
                android:id="@+id/fa_wifi"
                android:layout_width="18.14dp"
                android:layout_height="13.36dp"
                android:layout_marginStart="5dp"
                android:background="@drawable/fa_wifi"
                app:layout_constraintBottom_toBottomOf="@+id/g"
                app:layout_constraintStart_toEndOf="@+id/g"
                app:layout_constraintTop_toTopOf="parent" />

            <!-- Group -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/group"
                android:layout_width="22.91dp"
                android:layout_height="11.46dp"
                app:layout_constraintBottom_toBottomOf="@+id/fa_wifi"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/fa_wifi"
                app:layout_constraintTop_toTopOf="@+id/fa_wifi">
                <!-- Vector -->
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/battery_frame"
                    android:layout_width="22.91dp"
                    android:layout_height="11.46dp"
                    android:background="@drawable/vector__battery"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <!-- Vector -->
                    <View
                        android:id="@+id/vector"
                        android:layout_width="14.32dp"
                        android:layout_height="5.37dp"

                        android:layout_marginStart="2dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginEnd="4dp"
                        android:layout_marginBottom="1dp"
                        android:background="@drawable/vector__battery_level"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView

        android:layout_width="match_parent"
        android:layout_height="900dp"
        app:layout_constraintTop_toBottomOf="@+id/group_368">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mentro_profile_scrollvieww"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/group_591"
                android:layout_width="0dp"
                android:layout_height="162dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <!-- Group 563 -->
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/group_428"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="#00000000"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <!-- arrow-left 1 -->
                    <RelativeLayout
                        android:id="@+id/arrow_left_"
                        android:layout_width="20dp"
                        android:layout_height="16dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="9dp"
                        android:layout_marginBottom="8dp"
                        android:background="@drawable/arrow_left_"
                        android:clipToOutline="true"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <!-- Mentors -->
                    <TextView
                        android:id="@+id/mentors"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="12dp"
                        android:fontFamily="@font/mukta"
                        android:gravity="top"
                        android:text="@string/select_program"
                        android:textAppearance="@style/mentor_profile_name"
                        android:textFontWeight="600"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/arrow_left_"
                        app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>
                <!-- comment-circle-chat-message 1 -->

                <!-- Group 590 -->


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/group_590"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="31dp"
                    android:background="@drawable/group_51"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/group_428">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/cardView"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="13dp"
                        android:layout_marginTop="11dp"
                        android:layout_marginBottom="11dp"
                        app:cardCornerRadius="60dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <ImageView
                            android:id="@+id/mentor_profile_pic"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:src="@drawable/grace"></ImageView>
                    </androidx.cardview.widget.CardView>

                    <!-- Peculiar C. Umeh -->
                    <TextView
                        android:id="@+id/mentor_name"
                        android:layout_width="wrap_content"
                        android:layout_height="25dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="11dp"
                        android:fontFamily="@font/mukta"
                        android:gravity="top"
                        android:text="@string/peculiar_c_"
                        android:textAppearance="@style/peculiar_c_"
                        android:textColor="@color/mms_black_2"
                        android:textFontWeight="700"
                        app:layout_constraintStart_toEndOf="@+id/cardView"
                        app:layout_constraintTop_toTopOf="parent" />
                    <!-- Program Assistant, Andela, She/her -->
                    <TextView
                        android:id="@+id/bio_data"
                        android:layout_width="175dp"
                        android:layout_height="17dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:fontFamily="@font/mukta"
                        android:gravity="top"
                        android:text="@string/program_ass"
                        android:textAppearance="@style/program_ass"
                        android:textFontWeight="400"
                        app:layout_constraintStart_toStartOf="@+id/mentor_name"
                        app:layout_constraintTop_toBottomOf="@+id/mentor_name" />


                    <!-- Rectangle 11 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/rectangle_1"
                        android:layout_width="83dp"
                        android:layout_height="18dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="4dp"
                        android:background="@drawable/rectangle_11"
                        app:layout_constraintStart_toStartOf="@+id/bio_data"
                        app:layout_constraintTop_toBottomOf="@+id/bio_data">

                        <!-- PROGRAM ASST. -->
                        <TextView
                            android:id="@+id/Title"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="9dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginEnd="6dp"
                            android:fontFamily="@font/mukta"
                            android:gravity="top"
                            android:text="@string/mentro_title"
                            android:textAppearance="@style/mentor_title"
                            android:textColor="@color/mms_pry_2"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- Rectangle 11 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/rectangle_2"
                        android:layout_width="83dp"
                        android:layout_height="18dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="6dp"
                        android:background="@drawable/rectangle_11"
                        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
                        app:layout_constraintStart_toEndOf="@+id/rectangle_1"
                        app:layout_constraintTop_toTopOf="@+id/rectangle_1">

                        <!-- PROGRAM ASST. -->
                        <TextView
                            android:id="@+id/role"
                            android:layout_width="wrap_content"
                            android:layout_height="17dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="9dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginEnd="6dp"
                            android:gravity="top"
                            android:text="@string/mentor_gad"
                            android:textAppearance="@style/mentor_gad"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- comment-circle-chat-message 1 -->

                </androidx.constraintlayout.widget.ConstraintLayout>





                <!-- Oct 16, 2022 -->


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/frame_139"
                android:layout_width="0dp"
                android:layout_height="47dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="15dp"
                android:background="@drawable/frame_139"
                android:clipToOutline="true"
                app:layout_constraintEnd_toEndOf="@+id/group_591"
                app:layout_constraintStart_toStartOf="@+id/group_591"
                app:layout_constraintTop_toBottomOf="@+id/group_591">
                <!-- PDF -->
                <TextView
                    android:id="@+id/select_program"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="11dp"
                    android:layout_marginBottom="9dp"
                    android:gravity="center_vertical"
                    android:text="@string/select_program"
                    android:textAppearance="@style/dowload_as_file"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <!-- gridicons:dropdown -->
                <RelativeLayout
                    android:id="@+id/gridicons_d"
                    android:layout_width="38dp"
                    android:layout_height="38dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginEnd="10dp"
                    android:background="@drawable/arrow_dropdown_caret_sort_select_arrow_2"
                    android:clipToOutline="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>




            <!-- ACE 1 -->
            <View
                android:id="@+id/ace_1"
                android:layout_width="0dp"
                android:layout_height="260dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/certificates"
                app:layout_constraintEnd_toEndOf="@+id/group_591"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/frame_139" />





            <!-- Frame 351 -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/frame_351"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="105dp"
                android:background="@drawable/frame_351"
                android:clipToOutline="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/group_591"
                app:layout_constraintStart_toStartOf="@+id/group_591"
                app:layout_constraintTop_toBottomOf="@+id/ace_1">

                <!-- Download -->
                <TextView
                    android:id="@+id/download"
                    android:layout_width="79dp"
                    android:layout_height="30dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:gravity="top"
                    android:text="@string/generate"
                    android:textAppearance="@style/download_generate"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>